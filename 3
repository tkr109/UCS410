#1 

a<-pbinom(7,12,1/6)+pbinom(8,12,1/6)+pbinom(9,12,1/6)
print(a)

#2

a<-pnorm(84,mean=72,sd=15.2,lower.tail = FALSE)
print(a)


#3

a<-dpois(0,5)
print(a)


v<-c(48:50)
a<-sum(dpois(v,50))

print(a)


#4

a<-dhyper(3,17,250-17,5)
print(a)

#5
v<-(0:31)
pdf<-dbinom(v,31,0.447)
print(pdf)

plot(v,pdf)

cdf<-pbinom(v,31,0.447)
plot(v,cdf)


mean<-31*0.447
print(mean)

var<-mean*(1-0.447)
print(var)




